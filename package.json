{
  "name": "arcgis-react",
  "version": "1.0.5",
  "homepage": "https://github.com/am-maneaters/arcgis-react",
  "description": "React hook wrappers around the @arcgis/core library",
  "module": "./dist/index.js",
  "type": "module",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/am-maneaters/arcgis-react.git"
  },
  "scripts": {
    "dev": "vite",
    "build": "node --max_old_space_size=16384 ./node_modules/vite/bin/vite.js build --mode examples",
    "preview": "vite preview --mode examples",
    "build:lib": "vite build --mode library",
    "generate-layers": "npx ts-node-esm ./scripts/generate-layers.ts",
    "test": "vitest --mode library",
    "lint:fix": "eslint ./src/**/*.{ts,tsx} --fix",
    "postinstall": "cd ./examples/ && npm install && cd ../"
  },
  "keywords": [
    "arcgis",
    "hooks"
  ],
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/node": "^20.4.0",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unicorn": "^47.0.0",
    "jsdom": "^22.1.0",
    "prettier": "3.0.0",
    "react-dom": "^18.2.0",
    "rollup-plugin-copy": "^3.4.0",
    "typescript": "^5.1.6",
    "vite": "^4.4.2",
    "vite-plugin-dts": "^3.1.1",
    "vitest": "^0.33.0"
  },
  "peerDependencies": {
    "@arcgis/core": "^4.26 || ^4.27"
  },
  "author": "Sam Matenaer",
  "license": "Apache-2.0",
  "prettier": {
    "singleQuote": true
  },
  "dependencies": {
    "@arcgis/core": "^4.27",
    "react": "^18.2.0",
    "react-fast-compare": "^3.2.2"
  }
}
